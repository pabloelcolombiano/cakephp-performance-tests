{
    "name": "vierge-noire/cff-performance",
    "description": "Performance Analysis of the CakePHP Fixture Factories plugin.",
    "type": "project",
    "require": {
        "phpunit/phpunit": "^8.0",
        "josegonzalez/dotenv": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Pablo Ramirez",
            "email": "pabloelcolombiano@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "run-tests": "bash run_tests.sh"
    },
    "require-dev": {
        "pakacuda/cakephp-fixture-factories": "^2.0",
        "vierge-noire/cakephp-test-migrator": "^2.0"
    }
}
