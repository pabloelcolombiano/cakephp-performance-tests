{
    "name": "vierge-noire/cff-performance",
    "description": "Performance Analysis of the CakePHP Fixture Factories plugin.",
    "type": "project",
    "require": {
        "phpunit/phpunit": "^8.0",
        "josegonzalez/dotenv": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Pablo Ramirez",
            "email": "pabloelcolombiano@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "run-tests-mysql": "bash run_tests.sh Mysql",
        "run-tests-pgsql": "bash run_tests.sh Postgres",
        "run-tests-sqlite": "bash run_tests.sh Sqlite"
    },
    "require-dev": {
        "vierge-noire/cakephp-fixture-factories": "dev-next",
        "vierge-noire/cakephp-test-suite-light": "dev-trigger_based_sniffers",
        "vierge-noire/cakephp-test-migrator": "dev-next"
    }
}
